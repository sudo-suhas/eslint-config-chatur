{
  "name": "eslint-config-chatur",
  "version": "1.0.0",
  "description": "An eslint configuration for the astute",
  "main": "index.js",
  "engines": {
    "node": ">= 6"
  },
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "eslint .",
    "lint-staged": "lint-staged",
    "cz": "git-cz",
    "cz:retry": "git-cz --retry"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sudo-suhas/eslint-config-chatur.git"
  },
  "keywords": [
    "eslint",
    "eslintconfig",
    "shareable-configs",
    "best-practices",
    "eslint-config",
    "code-style",
    "javascript",
    "linter-config"
  ],
  "authors": [
    {
      "name": "Suhas Karanth",
      "email": "sudo.suhas@gmail.com",
      "url": "github.com/sudo-suhas"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sudo-suhas/eslint-config-chatur/issues"
  },
  "homepage": "https://github.com/sudo-suhas/eslint-config-chatur#readme",
  "lint-staged": {
    "**/*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "pre-commit": [
    "lint-staged",
    "test"
  ],
  "devDependencies": {
    "commitizen": "^2.9.6",
    "cz-conventional-changelog": "^2.0.0",
    "eslint": "^4.4.0",
    "eslint-config-prettier": "^2.3.0",
    "eslint-plugin-compat": "^1.0.4",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-lodash": "^2.4.4",
    "eslint-plugin-prettier": "^2.1.2",
    "eslint-plugin-promise": "^3.5.0",
    "lint-staged": "^4.0.3",
    "pre-commit": "^1.2.2",
    "prettier": "^1.5.3"
  },
  "peerDependencies": {
    "eslint": ">= 4.2.0",
    "eslint-config-prettier": ">= 2.3.0",
    "eslint-plugin-compat": ">= 1.0.4",
    "eslint-plugin-import": ">= 2.7.0",
    "eslint-plugin-lodash": ">= 2.4.4",
    "eslint-plugin-prettier": ">= 2.1.2",
    "eslint-plugin-promise": ">= 3.5.0",
    "prettier": ">= 1.5.3"
  },
  "dependencies": {
    "app-root-path": "^2.0.1",
    "eslint-plugin-eslint-comments": "^1.0.3",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-unicorn": "^2.1.2",
    "lodash": "^4.17.4"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
