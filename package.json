    "linter-config"
  ],
  "authors": [
    {
      "name": "Suhas Karanth",
      "email": "sudo.suhas@gmail.com",
      "url": "github.com/sudo-suhas"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sudo-suhas/eslint-config-chatur/issues"
  },
  "homepage": "https://github.com/sudo-suhas/eslint-config-chatur#readme",
